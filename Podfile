# Uncomment the next line to define a global platform for your project
platform :ios, '11.0'

target 'FUISample675' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for FUISample675
  pod 'FirebaseUI'

  # WORKAROUND: https://github.com/firebase/FirebaseUI-iOS/issues/673#issuecomment-488769795
  pod 'FBSDKCoreKit', '~> 4.0'
  pod 'FBSDKLoginKit', '~> 4.0'

  target 'FUISample675Tests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'FUISample675UITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
